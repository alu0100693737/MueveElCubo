

NTS_VAR_DEC -> NTS_FLOAT_DEC | NTS_VEC_DEC | NTS_INT_DEC
NTS_INT_DEC -> TS_INT NTS_ID TS_EQ NTS_INT_VAL
NTS_FLOAT_DEC -> TS_FLOAT NTS_ID TS_EQ NTS_FLOAT_VAL
NTS_VEC_DEC -> TS_VEC NTS_ID TS_EQ NTS_VEC_VAL
NTS_ID -> [_a-z][_a-z A-Z 0-9]*
NTS_INT_VAL -> \d+
NTS_FLOAT_VAL -> \d+(?:.\d+)?
NTS_VEC_VAL -> \(NTS_FLOAT_VAL,NTS_FLOAT_VAL,NTS_FLOAT_VAL\)

NTS_INST -> TS_INST0 | NTS_INST1VEC | NTS_INST1FLOAT
NTS_INST1VEC -> TS_INST1VEC TS_VEC
NTS_INST1FLOAT -> TS_INST1FLOAT TS_FLOAT